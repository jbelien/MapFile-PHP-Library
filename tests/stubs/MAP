MAP
  ANGLE 45
  CONFIG "ON_MISSING_DATA" "FAIL"
  CONFIG "PROJ_LIB" "/usr/local/share/proj/"
  DEBUG 5
  DEFRESOLUTION 72
  EXTENT 0 0 150000 150000
  FONTSET "fontset"
  IMAGECOLOR 0 255 0
  IMAGETYPE PNG
  LAYER
    NAME "my-layer"
  END # LAYER
  LEGEND
    IMAGECOLOR 255 0 0
  END # LEGEND
  MAXSIZE 4096
  NAME "my-map"
  OUTPUTFORMAT
    MIMETYPE "image/png; mode=8bit"
  END # OUTPUTFORMAT
  PROJECTION
    "init=epsg:4326"
  END # PROJECTION
  QUERYMAP
    COLOR 0 255 0
  END # QUERYMAP
  REFERENCE
    COLOR 255 0 0
  END # REFERENCE
  RESOLUTION 72
  SCALEBAR
    COLOR 0 0 0
  END # SCALEBAR
  SCALEDENOM 24000
  SHAPEPATH "./data"
  SIZE 1000 1000
  STATUS ON
  SYMBOL
    NAME "right-arrow"
  END # SYMBOL
  SYMBOLSET "symbolset"
  UNITS METERS
  WEB
    BROWSEFORMAT "text/html"
  END # WEB
END # MAP
