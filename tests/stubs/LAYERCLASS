CLASS
  DEBUG ON
  EXPRESSION ([POPULATION] > 50000 AND '[LANGUAGE]' eq 'FRENCH')
  GROUP "group1"
  KEYIMAGE "legend.png"
  LABEL
    COLOR 150 150 150
  END # LABEL
  LEADER
    MAXDISTANCE 30
  END # LEADER
  MAXSCALEDENOM 10000
  MINSCALEDENOM 1000
  NAME "name1"
  STATUS ON
  STYLE
    COLOR 80 80 80
  END # STYLE
  TEMPLATE "template.html"
  TEXT "[FIRSTNAME] [LASTNAME]"
  VALIDATION
    "firstname" "^[a-zA-Z\-]+$"
  END # VALIDATION
END # CLASS
