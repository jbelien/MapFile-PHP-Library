LABEL
  ALIGN CENTER
  ANGLE AUTO
  BUFFER 10
  COLOR 255 0 0
  EXPRESSION ([POPULATION] > 50000 AND '[LANGUAGE]' eq 'FRENCH')
  FONT "dejavu"
  FORCE TRUE
  MAXLENGTH 25
  MAXOVERLAPANGLE 45
  MAXSCALEDENOM 10000
  MAXSIZE 10
  MINDISTANCE 50
  MINFEATURESIZE 5
  MINSCALEDENOM 1000
  MINSIZE 1
  OFFSET 5 5
  OUTLINECOLOR 0 255 0
  OUTLINEWIDTH 1
  PARTIALS FALSE
  POSITION AUTO
  PRIORITY 10
  REPEATDISTANCE 5
  SHADOWCOLOR 0 0 255
  SHADOWSIZE 3 3
  SIZE LARGE
  STYLE
    COLOR 153 153 153
    GEOMTRANSFORM "labelcenter"
  END # STYLE
  STYLE
    COLOR 255 0 0
    GEOMTRANSFORM "labelpoly"
  END # STYLE
  TEXT "[FIRSTNAME] [LASTNAME]"
  TYPE TRUETYPE
  WRAP "-"
END # LABEL
